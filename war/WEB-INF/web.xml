  	</servlet-mapping>
  	<servlet>
		<servlet-name>MarkFunction</servlet-name>
	 	<servlet-class>com.winster.routemarks.rest.endpoint.Mark</servlet-class>
  	</servlet>
  	<servlet-mapping>
		<servlet-name>MarkFunction</servlet-name>
		<url-pattern>/markfn/*</url-pattern>
  	</servlet-mapping>
  	<servlet>
		<servlet-name>User</servlet-name>
