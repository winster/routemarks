	<servlet>
		<servlet-name>SystemServiceServlet</servlet-name>
		<servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
		<init-param>
			<param-name>services</param-name>
			<param-value/>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>SystemServiceServlet</servlet-name>
		<url-pattern>/_ah/spi/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SocialAuth</servlet-name>
	 	<servlet-class>com.winster.routemarks.rest.endpoint.SocialAuth</servlet-class>
  	</servlet>
  	<servlet-mapping>
		<servlet-name>SocialAuth</servlet-name>
		<url-pattern>/socialAuth/*</url-pattern>
  	</servlet-mapping>
  	<servlet>
		<servlet-name>GoogleAuth</servlet-name>
	 	<servlet-class>com.winster.routemarks.rest.endpoint.GoogleAuthServlet</servlet-class>
  	</servlet>
  	<servlet-mapping>
		<servlet-name>GoogleAuth</servlet-name>
		<url-pattern>/socialAuth/google</url-pattern>
  	</servlet-mapping>
  	<servlet>
		<servlet-name>GoogleAuthCallBack</servlet-name>
	 	<servlet-class>com.winster.routemarks.rest.endpoint.GoogleOAuth2Callback</servlet-class>
  	</servlet>
  	<servlet-mapping>
		<servlet-name>GoogleAuthCallBack</servlet-name>
		<url-pattern>/oauth2callback</url-pattern>
  	</servlet-mapping>
